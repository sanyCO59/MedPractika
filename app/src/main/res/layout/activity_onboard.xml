<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="#FFFFFF"
    tools:context=".Onboard">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/tab3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:visibility="gone"
        tools:visibility="visible">

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="287dp"
            android:layout_height="119dp"
            android:orientation="vertical"
            android:padding="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.356">

            <TextView
                android:id="@+id/textView23"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:fontFamily="@font/lato_bold"
                android:gravity="center_horizontal|top"
                android:text="Мониторинг"
                android:textColor="#00B712"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView56"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:fontFamily="@font/lato"
                android:gravity="center"
                android:text="Наши врачи всегда наблюдают
за вашими показателями здоровья"
                android:textColor="#939396" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView31"
            android:layout_width="142dp"
            android:layout_height="33dp"
            android:layout_margin="20sp"
            android:fontFamily="@font/lato_bold"
            android:onClick="next3"
            android:text="Завершить"
            android:textColor="#57A9FF"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/subtract" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
            app:srcCompat="@drawable/ill3" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/tab2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:visibility="gone">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="287dp"
            android:layout_height="119dp"
            android:orientation="vertical"
            android:padding="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.356">

            <TextView
                android:id="@+id/textView22"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:fontFamily="@font/lato_bold"
                android:gravity="center_horizontal|top"
                android:text="Уведомления"
                android:textColor="#00B712"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView32"
                android:layout_width="match_parent"
                android:layout_height="28dp"
                android:fontFamily="@font/lato"
                android:gravity="center"
                android:text="Вы быстро узнаете о результатах"
                android:textColor="#939396" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="142dp"
            android:layout_height="33dp"
            android:layout_margin="20sp"
            android:fontFamily="@font/lato_bold"
            android:onClick="next2"
            android:text="Пропустить"
            android:textColor="#57A9FF"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/subtract" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
            app:srcCompat="@drawable/ill2" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/tab1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        tools:visibility="gone">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="287dp"
            android:layout_height="119dp"
            android:orientation="vertical"
            android:padding="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.356">

            <TextView
                android:id="@+id/textView27"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:fontFamily="@font/lato_bold"
                android:gravity="center_horizontal|top"
                android:text="Анализы"
                android:textColor="#00B712"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="28dp"
                android:fontFamily="@font/lato"
                android:gravity="center"
                android:text="Экспресс сбор и получение проб"
                android:textColor="#939396" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView"
            android:layout_width="142dp"
            android:layout_height="33dp"
            android:layout_margin="20sp"
            android:fontFamily="@font/lato_bold"
            android:onClick="next1"
            android:text="Пропустить"
            android:textColor="#57A9FF"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/subtract" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            app:layout_constraintVertical_bias="0.502"
            app:srcCompat="@drawable/illustration1" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:layout_width="98dp"
        android:layout_height="25dp"
        android:layout_gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/check1"
            android:layout_width="18sp"
            android:layout_height="18sp"
            android:layout_weight="1"
            app:srcCompat="@drawable/ellipse1" />

        <ImageView
            android:id="@+id/check2"
            android:layout_width="18sp"
            android:layout_height="18sp"
            android:layout_weight="1"
            app:srcCompat="@drawable/ellipse2" />

        <ImageView
            android:id="@+id/check3"
            android:layout_width="18sp"
            android:layout_height="18sp"
            android:layout_weight="1"
            app:srcCompat="@drawable/ellipse2" />
    </LinearLayout>

</FrameLayout>